{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "buildClient": "webpack --config webpack.config.client.js --mode production",
    "start": "npm run buildClient && nodemon --exec babel-node src/server/index.js",
    "client": "webpack-dev-server --config webpack.config.client.js --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec babel-node --inspect --debug src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prettier": "prettier --write \"src/**/*.js\"",
    "flow": "flow",
    "updateLibsFlow": "flow-typed install",
    "yandexExamplesClient": "webpack-dev-server --config examples/YandexApiExamples/webpack.config.client.js --mode development --devtool inline-source-map --hot",
    "yandexExamplesServer": "nodemon --exec babel-node --inspect --debug examples/YandexApiExamples/server/index.js --watch examples/YandexApiExamples/server/index.js",
    "yandexExamples": "concurrently \"npm run yandexExamplesServer\" \"npm run yandexExamplesClient\"",
    "vkExamplesClient": "webpack-dev-server --config examples/VKApiExamples/webpack.config.client.js --mode development --devtool inline-source-map --hot",
    "vkExamplesServer": "nodemon --exec babel-node --inspect --debug examples/VKApiExamples/server/index.js --watch examples/VKApiExamples/server/index.js",
    "vkExamples": "concurrently \"npm run vkExamplesServer\" \"npm run vkExamplesClient\"",
    "infExamplesClient": "webpack-dev-server --config examples/InfrastructureExamples/webpack.config.client.js --mode development --devtool inline-source-map --hot",
    "infExamplesServer": "nodemon --exec babel-node --inspect --debug examples/InfrastructureExamples/server/index.js --watch examples/InfrastructureExamples/server/index.js",
    "infExamples": "concurrently \"npm run infExamplesServer\" \"npm run infExamplesClient\"",
    "mapPostsExamplesClient": "webpack-dev-server --config examples/MapPostsExamples/webpack.config.client.js --mode development --devtool inline-source-map --hot",
    "mapPostsExamplesServer": "nodemon --exec babel-node --inspect --debug examples/MapPostsExamples/server/index.js --watch examples/MapPostsExamples/server/**/*.js",
    "mapPostsExamples": "concurrently \"npm run mapPostsExamplesClient\" \"npm run mapPostsExamplesServer\""
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jsx-to-string": "^1.4.0",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-vkontakte": "^0.3.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-images": "^0.5.19",
    "react-photo-gallery": "^7.0.2",
    "react-router-dom": "^4.3.1",
    "react-scroll-to-component": "^1.0.2",
    "react-yandex-maps": "^3.0.2",
    "sharp": "^0.22.1",
    "shortid": "^2.2.14",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^3.0.3",
    "sqlite3": "^4.0.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "flow-bin": "^0.92.1",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "prettier": "1.16.4",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
